<h2>A New Lady Champ is Born</h2>
<% if flash[:errors] %>
<% flash[:errors].each do |error| %>
<ul>
  <li><%= error %></li>
</ul>
<% end %>
<% end %>

<%= form_for @heroine do |f| %>

<%= f.label :name, "name" %>
<%= f.text_field :name %>
<%= f.label :super_name, "Super Name" %>
<%= f.text_field :super_name %>
<%= f.collection_select(:power_id, @powers, :id, :name)%>
<%= f.submit %>
<%end %>
